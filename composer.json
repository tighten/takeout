{
    "name": "tightenco/takeout",
    "description": "Manage your dev dependencies with simple one-off Docker containers.",
    "keywords": ["cli", "console", "docker", "containers", "dependency management"],
    "homepage": "https://takeout.tighten.co/",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co"
        },
        {
            "name": "Jose Soto",
            "email": "jose@tighten.co"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.0",
        "laravel-zero/framework": "^8.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/laravel-console-menu": "^3.0",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "tightenco/tlint": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform-check": false,
        "platform": {
            "ext-pcntl": "7.2",
            "ext-posix": "7.2"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/takeout"]
}
